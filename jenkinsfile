pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        stage('Test') {
            steps {
                echo 'test'
            }
        }
        stage('deploy') {
            steps {
                echo 'deploy'
            }
        }
        stage('produccion') {
            steps {
                echo 'produccion'
            }
        }
        stage('release') {
            steps {
                echo 'release'
            }
        }
    }
}
